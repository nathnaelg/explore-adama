  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --no-warnings --loader ts-node/esm src/server.ts",
    "dev": "nodemon --ext ts,js,json --exec 'node --no-warnings --loader ts-node/esm' src/server.ts",
    "db:generate": "prisma generate --schema=src/database/prisma/schema.prisma",
    "db:format": "prisma format --schema=src/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=src/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/database/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=src/database/prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema=src/database/prisma/schema.prisma"
  },
